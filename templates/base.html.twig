<!DOCTYPE html>
<html>
{% block head %}

<head>
  <meta charset="UTF-8" />
  <title>
    {% block title %}
    Welcome!
    {% endblock %}
  </title>
  <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
  {% block stylesheets %}
  <style>
    a {
      color: black;
      text-decoration: none;
    }

    .link {
      color: black
    }

    .link:hover {
      color: black;
      text-decoration: none;
    }

    .image-container {
      width: 200px;
      height: 350px;
    }

    .image-container img {
      max-width: 100%;
      max-height: 100%;
      margin: auto;
      display: block;
    }

    .imageContainer {
      position: absolute;
      width: 554px;
      height: 127px;
      left: 1083px;
      top: 798px;

      background: #FEFEFE;
      border: 1px solid #000000;
    }
  </style>
  <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" rel="stylesheet" />
  {% endblock %}
</head>
{% endblock %}


<body>
  <nav class="navbar navbar-expand-md sticky-top navbar-light bg-white shadow-sm">
    <div class="container">
      <a class="navbar-brand d-flex" href="{{path('home')}}">
        <div>
          <img class="pr-3"
            src="data:image/jpeg;base64,"
            style="border-right: 1px solid #333; height: 50px" />
        </div>
        <div class="ml-3 pt-2">
          <h4>Instagram clone</h4>
        </div>
      </a>
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
        aria-controls="navbarSupportedContent" aria-expanded="false">
        <span class="navbar-toggler-icon"></span>
      </button>

      <div class="collapse navbar-collapse" id="navbarSupportedContent">
        <!-- Left Side Of Navbar -->
        <ul class="navbar-nav mr-auto"></ul>

        <!-- Right Side Of Navbar -->
        <ul class="navbar-nav ml-auto">
          <!-- Authentication Links -->
          {% if app.user %}
          <div class="mb-3">
            <a class="link navbar-brand" href="{{ path('app_logout') }}">Logout</a>
          </div>
          {% else %}
          <li class="nav-item">
            <a class="link" class="nav-link" href={{path('app_login')}}>
              <h4>Login</h4>
            </a>
          </li>
          <li class="nav-item ml-3">
            <a class="link" class="nav-link" href={{path('app_register')}}>
              <h4>Register</h4>
            </a>
          </li>
          {% endif %}
        </ul>
      </div>
    </div>
  </nav>
  {% block body %}

  {% endblock %}
  {% if app.user %}
  <footer class="footer">
    <div class="container">
      <nav class="navbar fixed-bottom navbar-light d-flex justify-content-center">
        <div class="col-md-2 ml-5"><a class="btn btn-light" style="background-color: transparent;"
            href="{{path('home')}}"><i class="fas fa-home fa-2x link"></i></a></div>
        <div class="col-md-2 ml-3"><i class="fas fa-search fa-2x"></i></div>
        <div class="col-md-2 ml-3"><a class="btn btn-light"
            style="background-color: transparent; border-color: trasparent;"
            href=" {{ path('post.new', {id: app.user.id, slug: app.user.getSlug()})}} "><i
              class=" fas fa-plus-square fa-2x spin"></i></a></div>
        <div class="col-md-2 ml-3"><i class="fas fa-heart fa-2x"></i></div>
        {% if Current_menu is defined and Current_menu == 'profile' and user == app.user %}
        <div class="col-md-2"><a class="btn btn-light" style="background-color: transparent;"
            href="{{ path('profile.edit', {id: app.user.id, slug: app.user.getSlug()})}} "><i
              class="fas fa-cog fa-2x spin"></i></a></div>
        {% else %}
        <div class="col-md-2"><a class="btn btn-light" style="background-color: transparent;"
            href="{{ path('profile.show', {id: app.user.id, slug: app.user.getSlug()})}} "><i
              class="fas fa-user fa-2x"></i></a>
        </div>
      </nav>
    </div>
  </footer>
  {% endif %}
  {% endif %}

  </nav>
  {% block javascripts %}
  <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js"></script>
  <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.bundle.min.js"></script>
  <script src="https://code.jquery.com/jquery-3.4.1.js" integrity="sha256-WpOohJOqMqqyKL9FccASB9O0KwACQJpFTUBLTYOVvVU="
    crossorigin="anonymous"></script>
  <script src="https://kit.fontawesome.com/43adf63c11.js" crossorigin="anonymous"></script>
  <script>
    $(".spin").hover(
      function () {
        $(this).addClass('fa-spin');
      },
      function () {
        $(this).removeClass('fa-spin');
      }
    );
  </script>
  {% endblock %}
</body>

</html>